{% extends "base.html.twig"%}

{% block body%}
   
    {% for post in allPosts %}
        {% if post.getSignaled() %}
            <div class= "post">
                <a href="{{ path('app_validate_post', {'postId' : post.getId()}) }}">Valider?</a>
                <p>{{post.getContent()}}</p>
            </div>
        {% endif %}
    {% endfor %} 

{% endblock %}